{
    "name": "informaticrevolution/product-bundle",
    "type": "symfony-bundle",
    "description": "Symfony IRProductBundle",
    "keywords": ["products"],
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Kirsch",
            "email": "informatic.revolution@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3.*",
        "symfony/form": "~2.3",
	"stof/doctrine-extensions-bundle": "1.1.*"
    },
    "require-dev": {
	"twig/twig": "~1.5",
        "doctrine/doctrine-bundle": "*",
	"symfony/yaml": "~2.3.*",
        "symfony/validator": "~2.3.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "IR\\Bundle\\ProductBundle": "" }
    },
    "target-dir": "IR/Bundle/ProductBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
